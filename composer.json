{
  "name": "extly/xtlaravelstarter",
  "type": "project",
  "description": "This project is a good starting point to begin using the Laravel Framework on Joomla.",
  "keywords": [
    "joomla",
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3 || ^8.0",
    "anibalsanchez/xt-laravel-starter-for-joomla-library": "dev-master",
    "mnsami/composer-custom-directory-installer": "^1.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "installer-paths": {
      "./library/": [
        "anibalsanchez/xt-laravel-starter-for-joomla-library"
      ]
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:anibalsanchez/xt-laravel-starter-for-joomla-library.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
